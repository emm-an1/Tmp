# What's the first signal node for oil?
signal_node = NodeRegistry.find("oil", node_type="PrecomputedUVTrendSignalNode")[0]
signal = NodeRegistry.get(signal_node)

print("Signal mv first date:", signal.mv().index[0])
print("Signal mv first VALID (non-zero/non-NaN):", signal.mv().replace(0, float('nan')).first_valid_index())
print("Signal pnl first valid:", signal.pnl().first_valid_index())
