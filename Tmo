trend_node = NodeRegistry.get("oil_complex_trend")
ws_node = NodeRegistry.get("oil_complex_trend_ws")

# Check pnls going into correlation calc
fast_pnl = NodeRegistry.get("oil_complex_fast").pnl()
med_pnl = NodeRegistry.get("oil_complex_medium").pnl()
slow_pnl = NodeRegistry.get("oil_complex_slow").pnl()

print("Fast pnl first valid:", fast_pnl.first_valid_index())
print("Medium pnl first valid:", med_pnl.first_valid_index())
print("Slow pnl first valid:", slow_pnl.first_valid_index())

# Check NaNs
print("\nNaN counts:")
print("Fast:", fast_pnl.isna().sum().values[0])
print("Medium:", med_pnl.isna().sum().values[0])
print("Slow:", slow_pnl.isna().sum().values[0])
